{
  "name": "aptos-conflict-guard-backend",
  "version": "1.0.0",
  "description": "Backend for Aptos Conflict Guard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "npm run build && NODE_ENV=development nodemon -r dotenv/config --exec \"node -r dotenv/config\" dist/index.js",
    "build": "tsc",
    "seed": "npm run build && node -r dotenv/config dist/seeders/demoUser.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.23.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.11.2",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
